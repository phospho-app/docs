  "navigation": [
    {
      "group": "Get Started",
      "pages": ["introduction", "quickstart"]
    },
    {
      "group": "Features",
      "pages": ["features/logging", "features/evaluation", "features/user-feedback", "features/events"]
    },
    {
      "group": "Other",
      "pages": ["other/faq"]
    },
    {
      "group": "Examples",
      "pages": ["examples/openai-agent", "examples/openai-streamlit"]
    },
    {
      "group": "API Documentation",
      "pages": ["api-reference/introduction"]
    },
    {
      "group": "Endpoint Examples",
      "pages": [
        "api-reference/endpoint/get",
        "api-reference/endpoint/create",
        "api-reference/endpoint/delete"
      ]
    }
  ],
  "footerSocials": {
    "twitter": "https://twitter.com/phospho_app",
    "github": "https://github.com/phospho-app",
    "linkedin": "https://www.linkedin.com/company/phospho-app/"
  }

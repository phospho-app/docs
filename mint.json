{
  "$schema": "https://mintlify.com/schema.json",
  "name": "phospho",
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#00FF00",
    "light": "#00FF00",
    "dark": "#1E1E1E",
    "anchors": {
      "from": "#00FF00",
      "to": "#07C983"
    }
  },
  "modeToggle": {
    "default": "dark",
    "isHidden": true
  },
  "topbarLinks": [
    {
      "name": "Support",
      "url": "mailto:contact@phospho.app"
    }
  ],
  "topbarCtaButton": {
